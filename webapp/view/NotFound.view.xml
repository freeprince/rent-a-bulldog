<mvc:View
      controllerName="rab.controller.NotFound"
      xmlns="sap.m"
      xmlns:mvc="sap.ui.core.mvc">
      <FlexBox
            width="100%"
            height="100%"
            alignItems="Center"
            justifyContent="Center">
            <Image src="./images/waldo.png"/>
            <VBox class="sapUiSmallMargin">
                  <Label text="{i18n>NotFound}" class="bigLabel" />
                  <Label text="{i18n>NotFound.text}" />
                  <Label text="{i18n>NotFound.description}" />
                  <Link text="Start over" press="navToHome"/>
            </VBox>
      </FlexBox>
</mvc:View>